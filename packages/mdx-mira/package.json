    "lint": "eslint . --ext .ts,tsx",
    "lint:fix": "eslint . --ext .ts,tsx --fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@mirajs/transpiler-esbuild": "workspace:*",
    "@mirajs/util": "workspace:*",
    "mdast-util-to-hast": "^12.1.0",
    "unist-util-visit": "^4.0.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^2.0.0",
    "unified": "^10.0.0",
    "unist-util-inspect": "^7.0.0"
  }
}
