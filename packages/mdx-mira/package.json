{
  "name": "@mirajs/mdx-mira",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "run-s build:*",
