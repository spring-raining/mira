{
  "name": "@mirajs/wui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.es.js"
    },
    "./styles.css": "./lib/style.css"
  },
  "types": "./lib/module/index.d.ts",
  "scripts": {
    "prebuild": "shx rm -rf dist lib",
    "dev": "vite",
    "build": "run-s build:module:vite build:module:types",
    "build:module:vite": "tsc && vite build",
    "//": "Run tsc twice. It seems to need the buildinfo file",
    "build:module:types": "run-s -c build:module:types:1 build:module:types:2",
    "build:module:types:1": "tsc -p tsconfig.module.json --tsBuildInfoFile tsconfig.tsbuildinfo --emitDeclarationOnly --outDir lib 2>/dev/null || exit 0",
    "build:module:types:2": "tsc -p tsconfig.module.json --tsBuildInfoFile tsconfig.tsbuildinfo --emitDeclarationOnly --outDir lib",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,tsx",
    "lint:fix": "eslint . --ext .ts,tsx --fix"
  },
  "dependencies": {
    "@mdx-js/react": "2.0.0-next.9",
    "@mirajs/core": "1.0.0",
    "@mirajs/transpiler": "1.0.0",
    "hast-to-hyperscript": "9.0.1",
    "mdast-util-mdx": "0.1.1",
    "monaco-editor": "0.21.2",
    "nanoid": "3.1.20",
    "react-dnd": "14.0.3",
    "react-dnd-html5-backend": "14.0.1",
    "recoil": "0.6.1"
  },
  "devDependencies": {
    "@chakra-ui/react": "1.6.0",
    "@monaco-editor/react": "^4.0.10",
    "@popperjs/core": "2.10.2",
    "@types/cssesc": "3.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vanilla-extract/css": "1.6.8",
    "@vanilla-extract/recipes": "0.2.3",
    "@vanilla-extract/sprinkles": "1.3.3",
    "@vanilla-extract/vite-plugin": "3.1.2",
    "@vitejs/plugin-react": "1.0.7",
    "clsx": "1.1.1",
    "event-target-shim": "6.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "prism-react-renderer": "^1.2.1",
    "react-container-query": "0.12.0",
    "react-intersection-observer": "^8.32.0",
    "requestidlecallback-polyfill": "^1.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "vite": "2.7.2"
  },
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  }
}
