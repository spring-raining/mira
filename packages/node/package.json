{
  "name": "@mirajs/cli",
  "version": "1.0.0",
  "type": "module",
  "module": "./module/index.mjs",
  "types": "./module/index.d.ts",
  "bin": {
    "mira": "bin/mira.js"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.22.0"
  },
  "scripts": {
    "prebuild": "shx rm -rf lib",
    "build": "run-p build:*",
    "build:rollup": "rollup -c",
    "build:types": "tsc -p tsconfig.module.json --emitDeclarationOnly --outDir lib",
    "predev": "shx rm -rf lib",
    "dev": "run-p dev:*",
    "dev:cli": "rollup -c -w --no-watch.clearScreen --environment DEV",
    "dev:wui": "yarn workspace @mirajs/wui vite build -w",
    "lint": "eslint . --ext .ts,tsx",
    "lint:fix": "eslint . --ext .ts,tsx --fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "3.0.1",
    "@types/babel__code-frame": "^7.0.2",
    "@types/command-line-usage": "^5.0.1",
    "@types/debounce": "^1.2.0",
    "@types/ip": "^1.1.0",
    "@types/koa-send": "4.1.3",
    "@types/picomatch": "^2.2.4"
  },
  "dependencies": {
    "@mirajs/workspace": "1.0.0",
    "@msgpack/msgpack": "2.7.1",
    "@web/dev-server": "^0.1.14",
    "@web/dev-server-hmr": "^0.1.9",
    "es-module-lexer": "^0.4.1",
    "esbuild": "^0.11.9",
    "esm": "^3.2.25",
    "globby": "^11.0.2",
    "koa-send": "5.0.1",
    "picomatch": "^2.2.2",
    "reflect-metadata": "0.1.13",
    "snowpack": "3.4.0",
    "vite": "2.7.2"
  }
}
